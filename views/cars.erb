<div class="row text-center">
  <h2>All Available Cars</h2>
</div>


<ul>

  <% @dealerships.each do | dealership | %>

    <% dealership.cars.each do | car | %>

      <li><a href="/dealerships/<%= dealership.id %>/cars/<%= car.id %>"><%= car.make %> <%= car.model %> <%= car.year %></a></li>

    <% end %>

  <% end %>

</ul>
